function Beh_new(f_name,tr,num_run,num_cdf)
v = xlsread(f_name,1); 
v = sort(v,'descend'); 
tr = sort(tr,'descend');
n = length(tr);
d = {};
for i =1:length(v)
 d{i,1} = v(i);
end


